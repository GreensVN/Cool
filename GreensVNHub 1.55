local minimized = false
local prevSize, prevPosition
local baseBtnSize = 44
local floatBtnSize = baseBtnSize * 1.25
local floatBtn = Instance.new("TextButton")
floatBtn.Size = UDim2.new(0, floatBtnSize, 0, floatBtnSize)
