-- ========== H√ÄM T·∫†O CARD SCRIPT ==========

local function createScriptCard(data)
    local card = Instance.new("Frame")
    card.Size = UDim2.new(1, 0, 0, 90)
    card.BackgroundColor3 = Color3.fromRGB(49, 54, 75)
    card.BorderSizePixel = 0
    card.ZIndex = 3
    local cardCorner = Instance.new("UICorner", card)
    cardCorner.CornerRadius = UDim.new(0.34, 0)

    -- icon (n·∫øu c√≥)
    if data.icon then
        local icon = Instance.new("ImageLabel")
        icon.Image = data.icon
        icon.Size = UDim2.new(0, 62, 0, 62)
        icon.Position = UDim2.new(0, 12, 0, 14)
        icon.BackgroundTransparency = 1
        icon.ZIndex = 3
        icon.Parent = card
        local icCor = Instance.new("UICorner", icon)
        icCor.CornerRadius = UDim.new(0.5,0)
    end

    -- T√™n script
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Text = data.name
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextSize = 20
    nameLabel.TextColor3 = Color3.fromRGB(120,255,120)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Size = UDim2.new(1, -200, 0, 28)
    nameLabel.Position = UDim2.new(0, 84, 0, 10)
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.Parent = card

    -- M√¥ t·∫£ script
    local descLabel = Instance.new("TextLabel")
    descLabel.Text = data.desc
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextSize = 16
    descLabel.TextColor3 = Color3.fromRGB(200,230,255)
    descLabel.BackgroundTransparency = 1
    descLabel.Size = UDim2.new(1, -200, 0, 32)
    descLabel.Position = UDim2.new(0, 84, 0, 38)
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.TextYAlignment = Enum.TextYAlignment.Top
    descLabel.TextWrapped = true
    descLabel.Parent = card

    -- Category
    local catLabel = Instance.new("TextLabel")
    catLabel.Text = data.category or "Kh√°c"
    catLabel.Font = Enum.Font.Gotham
    catLabel.TextSize = 14
    catLabel.TextColor3 = Color3.fromRGB(255,225,120)
    catLabel.BackgroundTransparency = 1
    catLabel.Position = UDim2.new(1, -104, 0, 10)
    catLabel.Size = UDim2.new(0, 90, 0, 22)
    catLabel.TextXAlignment = Enum.TextXAlignment.Right
    catLabel.Parent = card

    -- N√∫t Copy
    local copyBtn = Instance.new("TextButton")
    copyBtn.Text = "Copy"
    copyBtn.Size = UDim2.new(0, 66, 0, 32)
    copyBtn.Position = UDim2.new(1, -74, 1, -42)
    copyBtn.BackgroundColor3 = Color3.fromRGB(120,170,255)
    copyBtn.TextColor3 = Color3.new(1,1,1)
    copyBtn.Font = Enum.Font.GothamBold
    copyBtn.TextSize = 17
    copyBtn.ZIndex = 5
    copyBtn.Parent = card
    local cbCorner = Instance.new("UICorner", copyBtn)
    cbCorner.CornerRadius = UDim.new(0.5, 0)
    copyBtn.MouseButton1Click:Connect(function()
        setclipboard(data.url)
        copyBtn.Text = "ƒê√£ copy!"
        wait(1)
        copyBtn.Text = "Copy"
    end)

    -- N√∫t Ch·∫°y script (ki·ªÉm tra l·ªói loadstring)
    local execBtn = Instance.new("TextButton")
    execBtn.Text = "Ch·∫°y"
    execBtn.Size = UDim2.new(0, 66, 0, 32)
    execBtn.Position = UDim2.new(1, -150, 1, -42)
    execBtn.BackgroundColor3 = Color3.fromRGB(90, 200, 120)
    execBtn.TextColor3 = Color3.new(1,1,1)
    execBtn.Font = Enum.Font.GothamBold
    execBtn.TextSize = 17
    execBtn.ZIndex = 5
    execBtn.Parent = card
    local ebCorner = Instance.new("UICorner", execBtn)
    ebCorner.CornerRadius = UDim.new(0.5, 0)
    execBtn.MouseButton1Click:Connect(function()
        execBtn.Text = "ƒêang t·∫£i..."
        local ok, s = pcall(loadstring, game:HttpGet(data.url))
        if ok and typeof(s) == "function" then
            local run, runerr = pcall(s)
            if run then
                execBtn.Text = "OK!"
            else
                execBtn.Text = "L·ªói!"
                warn("[LIBRARY] Script l·ªói khi ch·∫°y: "..tostring(runerr))
            end
        else
            execBtn.Text = "Load l·ªói!"
            warn("[LIBRARY] Script t·∫£i v·ªÅ b·ªã l·ªói ho·∫∑c kh√¥ng ph·∫£i function: "..tostring(s))
        end
        wait(1)
        execBtn.Text = "Ch·∫°y"
    end)

    -- Hi·ªáu ·ª©ng hover card
    card.MouseEnter:Connect(function()
        card.BackgroundColor3 = Color3.fromRGB(60,68,100)
    end)
    card.MouseLeave:Connect(function()
        card.BackgroundColor3 = Color3.fromRGB(49, 54, 75)
    end)

    return card
end

-- ========== H√ÄM L·ªåC & C·∫¨P NH·∫¨T LIST ==========

function updateLibraryList()
    for _, v in pairs(libScroll:GetChildren()) do
        if v:IsA("Frame") then v:Destroy() end
    end
    local query = searchBox.Text:lower()
    local selCat = dropCat.Text:match("^(.-) ‚ñº$") or "T·∫•t c·∫£"
    local count = 0
    for _, data in ipairs(scriptLibrary) do
        local matchCat = (selCat == "T·∫•t c·∫£") or (data.category == selCat)
        local matchSearch = (data.name:lower():find(query) or (data.desc and data.desc:lower():find(query)) or (data.category and data.category:lower():find(query)))
        if matchCat and matchSearch then
            createScriptCard(data).Parent = libScroll
            count = count + 1
        end
    end
    scriptCountLabel.Text = "T·ªïng s·ªë script: " .. count .. "/" .. #scriptLibrary
    libScroll.CanvasSize = UDim2.new(0,0,0,libLayout.AbsoluteContentSize.Y+18)
end
searchBox:GetPropertyChangedSignal("Text"):Connect(updateLibraryList)
updateLibraryList()

-- ========== K·∫æT N·ªêI TAB ==========
for i, btn in ipairs(tabBtns) do
    btn.MouseButton1Click:Connect(function()
        for j, frame in ipairs(tabFrames) do
            frame.Visible = (j == i)
            tabBtns[j].BackgroundColor3 = (j == i) and Color3.fromRGB(64, 110, 64) or Color3.fromRGB(55, 55, 65)
        end
    end)
end

