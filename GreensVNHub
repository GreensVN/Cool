    btn.BackgroundColor3 = Color3.fromRGB(70, 90, 80)
    btn.Text = btnNames[i]
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 20
    btn.Name = btnNames[i]
    btn.Parent = scroll
